{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "name": "Student",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "name": {
      "type": "string",
      "regex": "^([A-Za-z]+)\\s*([A-Za-z]*)\\s+([A-Za-z])\\.$"
    },
    "character": {
      "type": "string"
    },
    "classroom": {
      "via": "students",
      "model": "classroom"
    },
    "sessions": {
      "via": "students",
      "collection": "session",
      "dominant": true
    },
    "enrolled": {
      "type": "boolean",
      "default": true,
      "required": true
    },
    "last_logged_in": {
      "type": "datetime"
    },
    "deleted": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "time_deleted": {
      "type": "datetime",
      "default": "2001-01-01T05:00:00.000Z"
    }
  }
}
